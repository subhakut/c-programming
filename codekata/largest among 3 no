#include <stdio.h>

int main()
{
    int a,b,c;
    printf("\nenter the first number: ");
    scanf("%d",&a);
    printf("\nenter the second number:");
    scanf("%d",&b);
    printf("\nenter the third number:");
    scanf("%d",&c);
    if(a>b&&a>c)
    {
        printf("a=%d is greater",a);
    }
    else if(b>c&&b>a)
    {
        printf("b=%d is greater",b);

    }
    else
    {
        printf("c=%d is greater",c);
    }


    return 0;
}
